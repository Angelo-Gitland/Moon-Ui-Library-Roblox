local MoonLib = loadstring(game:HttpGet("https://gist.githubusercontent.com/Angelo-Gitland/3677d310efab712378a1ec236836a03d/raw/5328e08a946007d08f320793c9895d03ab22149f/moon%2520ui"))() -- Updated link

local Window = MoonLib:CreateWindow({
    Title = "Moon Ui Library", -- Default title, You can customize this
    Icon = "Moon", -- Available icons see all here: 
    Subtitle = "Developed By Lua Land Team", -- Default subtitle, You can customize this
    Theme = "default", -- Default is black,  Available themes see here: 
    IntroText = "Moon Ui Library", -- Default, the text that you will see in intro
    IntroDesc = "The easiest way", -- Default, the description of the intro
    IntroIcon = "Moon" -- Default, the icon of the Intro while initializing Available icons see here: 
})

local Home = Window:CreatePage("Home", "Home") -- Page title & Icon
Home:CreateSection("HOME PAGE") -- Section & Text
Home:CreateTextLabel("TEXTLABEL (HOMEPAGE)") -- TextLabel & Text
Home:CreateTextButton("Button Title", function()
    print("Your loadstring script here")
end) -- TextButton & Function

local Main = Window:CreatePage("Main", "Code-Xml") -- Page title & Icon
Main:CreateSection("Main Page Customize this") -- Section & Text
Main:CreateToggle("Enable Feature", function(state)
    print("If enabled, what happens? place the function or loadstring code here")
end)
Main:CreateTextBox("Input Value", function(text)
    print("For example, You want to set your speed into 90 so you type the value.Place your logic here")
end)

local Extra = Window:CreatePage("Extra", "box") -- Page title & Icon
Extra:CreateSection("Additional Features") -- Section & Text
Extra:CreateTextLabel("Extra Features Place any buttons here") -- TextLabel & Text

local Credits = Window:CreatePage("Credits", "Profile") -- Page title & Icon
Credits:CreateSection("Developers")
Credits:CreateDropdown("Developers", {
    "Eric_3337", --)
    "LLH Lua Land", -- Button 2 (Dropdown)
    "Lua Land Team", -- Button 3 (Dropdown)
}, function(selected)
    print("Selected developer:", selected)
end)

Credits:CreateSection("DISCORD")
Credits:CreateTextButton("Copy Discord Link", function()
    -- Send Notification > Set link to clipboard (If you want, Place your logic here)
    print("Discord link copied to clipboard")
end)
